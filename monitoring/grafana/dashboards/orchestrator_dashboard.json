{
  "title": "RSS Orchestrator SLOs",
  "uid": "rss-orchestrator",
  "schemaVersion": 38,
  "version": 2,
  "timezone": "browser",
  "refresh": "30s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Throughput (req/min)",
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
      "fieldConfig": {"defaults": {"unit": "reqpm"}},
      "targets": [
        {
          "expr": "sum(rate(rss_orchestrator_requests_total[$__interval])) by (command) * 60",
          "legendFormat": "{{command}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency p50/p95/p99 (s)",
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8},
      "fieldConfig": {"defaults": {"unit": "s"}},
      "targets": [
        {"expr": "histogram_quantile(0.5, sum(rate(rss_orchestrator_latency_seconds_bucket[$__interval])) by (command, le))", "legendFormat": "p50 {{command}}"},
        {"expr": "histogram_quantile(0.95, sum(rate(rss_orchestrator_latency_seconds_bucket[$__interval])) by (command, le))", "legendFormat": "p95 {{command}}"},
        {"expr": "histogram_quantile(0.99, sum(rate(rss_orchestrator_latency_seconds_bucket[$__interval])) by (command, le))", "legendFormat": "p99 {{command}}"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Error Rate (%)",
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16},
      "fieldConfig": {"defaults": {"unit": "percentunit"}},
      "targets": [
        {
          "expr": "100 * (sum(rate(rss_orchestrator_errors_total[$__interval])) by (command) / clamp_min(sum(rate(rss_orchestrator_requests_total[$__interval])) by (command), 1))",
          "legendFormat": "{{command}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Evidence Coverage (%)",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
      "fieldConfig": {"defaults": {"unit": "percentunit"}},
      "targets": [
        {"expr": "rss_orchestrator_evidence_coverage_ratio * 100", "legendFormat": "{{command}}"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Cost per Command (USD)",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
      "fieldConfig": {"defaults": {"unit": "currencyUSD", "decimals": 2}},
      "targets": [
        {"expr": "histogram_quantile(0.95, sum(rate(rss_command_cost_cents_bucket[$__interval])) by (command, le)) / 100", "legendFormat": "p95 {{command}}"}
      ]
    },
    {
      "type": "bargauge",
      "title": "Error Types (per minute)",
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32},
      "fieldConfig": {"defaults": {"unit": "reqpm"}},
      "options": {"displayMode": "lcd"},
      "targets": [
        {
          "expr": "sum(rate(rss_orchestrator_errors_total[$__interval])) by (reason)",
          "legendFormat": "{{reason}}"
        }
      ]
    }
  ],
  "templating": {
    "list": [
      {
        "name": "command",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(rss_orchestrator_requests_total, command)",
        "refresh": 1
      }
    ]
  }
}
